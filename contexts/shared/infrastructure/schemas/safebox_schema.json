{
  "type": "object",
  "properties": {
    "data": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "uuid",
          "example": "f626c808-648c-41fe-865d-c6062f3e0899"
        },
        "type": {
          "type": "string",
          "example": "safebox"
        },
        "attributes": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string",
              "example": "Secure safebox 01"
            }
          },
          "required": ["name"]
        },
        "links": {
          "type": "object",
          "properties": {
            "self": {
              "$ref": "#/components/schemas/create_safebox_link"
            },
            "open": {
              "$ref": "#/components/schemas/open_safebox_link"
            },
            "getItems": {
              "$ref": "#/components/schemas/list_safebox_items_link"
            },
            "addItem": {
              "$ref": "#/components/schemas/add_safebox_item_link"
            }
          },
          "required": ["self"]
        }
      },
      "required": ["id", "type", "attributes", "links"]
    }
  },
  "required": ["data"]
}
